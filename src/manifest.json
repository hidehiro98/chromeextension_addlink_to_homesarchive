{
  "name": "Yahoo Realestate to Home's Archive",
  "version": "0.9.0",
  "manifest_version": 2,
  "description": "Add a shortcut to the Home's Archive in the Yahoo Realestate",
  "content_scripts": [{
    "matches": ["https://realestate.yahoo.co.jp/rent/*"],
    "js": [
      "content.js"
    ]
  }],
  "permissions": [
    "webRequest",
    "*://*.google.com/"
  ],
  "background": {
    "scripts": ["hot-reload.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons" : {
    "128": "icon.png"
  }
}
